    <eStructuralFeatures xsi:type="ecore:EReference" name="qualityRequirements" upperBound="-1"
        eType="#//QualityRequirement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expression">
